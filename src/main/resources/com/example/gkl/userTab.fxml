<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="383.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gkl.fxControllers.UserController">
    <children>
        <TableView fx:id="customerTable" layoutX="14.0" layoutY="58.0" prefHeight="146.0" prefWidth="578.0">
            <columns>
                <TableColumn fx:id="customerIdCol" prefWidth="42.0" text="ID" />
                <TableColumn fx:id="customerLoginCol" prefWidth="85.0" text="Login" />
                <TableColumn fx:id="customerPasswordCol" prefWidth="85.0" text="Password" />
                <TableColumn fx:id="customerContactMailCol" prefWidth="85.0" text="Contact Mail" />
                <TableColumn fx:id="customerBirthDateCol" prefWidth="85.0" text="Birth Date" />
                <TableColumn fx:id="customerFirstNameCol" prefWidth="85.0" text="First Name" />
                <TableColumn fx:id="customerLastNameCol" prefWidth="85.0" text="Last Name" />
            </columns>
        </TableView>
        <TableView fx:id="managerTable" layoutX="14.0" layoutY="213.0" prefHeight="144.0" prefWidth="578.0">
            <columns>
                <TableColumn fx:id="managerIdCol" prefWidth="42.0" text="ID" />
                <TableColumn fx:id="managerLoginCol" prefWidth="108.0" text="Login" />
                <TableColumn fx:id="managerPasswordCol" prefWidth="75.0" text="Password" />
                <TableColumn fx:id="managerContactMailCol" text="Contact Mail" />
                <TableColumn fx:id="managerBirthDateCol" prefWidth="79.0" text="Birth Date" />
                <TableColumn fx:id="managerFirstNameCol" prefWidth="83.0" text="First Name" />
                <TableColumn fx:id="managerLastNameCol" prefWidth="88.0" text="Last Name" />
            </columns>
        </TableView>
        <Button fx:id="addUserButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#createUser" text="Add" />
        <Button fx:id="deleteUserButton" layoutX="82.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete" />
    </children></AnchorPane>
